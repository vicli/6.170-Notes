<h1>Listing webs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Visits</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @webs.each do |web| %>
  <tr>
    <td><%= web.name %></td>
    <td><%= web.visits %></td>
    <td><%= link_to 'Show', web %></td>
    <td><%= link_to 'Visit', edit_web_path(web) %></td>
    <td><%= link_to 'Destroy', web, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Web', new_web_path %>
