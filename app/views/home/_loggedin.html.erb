<div id = "head" >
	<h2>TASKED </h2>
	</div>
	<div class="login">
	<%= form_for("user", :url => user_session_path) do |f| %>
	  <p> Email:    <span><%= f.text_field :email %> </span></p>
	  
	  <p> Password: <%= f.password_field :password %> </p>
	  <p><%= f.check_box :remember_me %><%= f.label :remember_me %></p>
	  <p id = "but"><%= f.submit 'Sign in' %></p>
	<% end %>
	</div>